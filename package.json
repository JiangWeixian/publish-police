{
  "name": "publish-police",
  "version": "0.0.1",
  "description": "check npm dist before publish",
  "keywords": [
    "npm",
    "cli",
    "prepublish",
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JiangWeixian/publish-police#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/publish-police.git"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/publish-police/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "bin": {
    "bin-template": "bin/index.mjs"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c --silent -w",
    "vitest": "vitest",
    "test": "pnpm run build && pnpm run vitest",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:version": "pnpm changeset version",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "prepublishOnly": "./bin/index.mjs",
    "prepare": "husky install",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cac": "^6.7.12",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.5.1",
    "@changesets/cli": "^2.19.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "17.0.8",
    "@types/rimraf": "^3.0.2",
    "cz-emoji": "^1.3.1",
    "esbuild": "^0.14.10",
    "eslint": "~8.22.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "picocolors": "1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-size": "^0.2.2",
    "type-fest": "^2.18.0",
    "typescript": "^4.5.4",
    "vitest": "^0.22.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  }
}
